	</tr>
	</thead>
	<tbody>
	'''
	# report leaf wetness periods
	'<TR><td class=red_text colspan=7 height=21>Download Time: %d/%d/%d %d:00</td></tr>'%\
	   (smry_dict['last_time'][1],smry_dict['last_time'][2],smry_dict['last_time'][0],smry_dict['last_time'][3])
