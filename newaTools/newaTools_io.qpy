# help page
def helppage [html] (urls):		
	if urls: 
		'<h3>Helpful Information</h3>'
		'<ul>'
		for desc,url in urls:
			if len(url) > 0:
				'<li><a href="%s" target="_blank">%s</a></li>' % (url,desc)
			else:
				'<li>%s</li>' % (desc)
		'</ul>'
	else:
		'<p>No help available at this time.</p>'
		
